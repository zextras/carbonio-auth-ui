		"@babel/preset-react": "^7.18.6",
		"@babel/preset-typescript": "^7.21.0",
		"@commitlint/cli": "^17.4.3",
		"@commitlint/config-conventional": "^17.4.3",
		"@types/history": "^4.7.11",
